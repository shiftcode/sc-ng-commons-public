{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "es2017",
    "module": "commonjs",
    "lib": ["es2016", "esnext.asynciterable"],
    "strict": true,
    "strictPropertyInitialization": false,
    "sourceMap": true,
